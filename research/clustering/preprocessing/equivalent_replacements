0-9 -> \d
  -> \s
[0123456789] -> \d
(?=\,\d*\}) r->r 0
{1,} -> +
{0,} -> *
{0,1} -> ?
http|https -> https?
(s)? -> s?
[s]? -> s?
00|0 -> 00?
(,)? -> ,?
\p{L} -> \w
\p{Letter} -> \w
\p{Z} -> \s
\p{Separator} -> \s
\p{N} -> \d
\p{Number} -> \d
[^\w] -> \W
[^\d] -> \D
\?P\<[^>]*> r->r <NAMEGROUP>
\?\<[^>]*> r->r <NAMEGROUP>
\?\'[^']*' r->r <NAMEGROUP>